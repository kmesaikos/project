{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
            // exclude .js and .js.map files, when in a TypeScript project
            "**/*.js": { "when": "$(basename).ts"},
            "**/*.js.map": true
    }
}